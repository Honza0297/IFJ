a = 1
if a > 2 then
print "spatne\n"
else
print "ok\n"
end

if a < 2 then
print "ok\n"
else
print "spatne\n"
end

if a == 1 then
print "ok\n"
else
print "spatne\n"
end

if a == 2 then
print "spatne\n"
else
print "ok\n"
end

if a != 1 then
print "spatne\n"
else
print "ok\n"
end

if a != 2 then
print "ok\n"
else
print "spatne\n"
end
